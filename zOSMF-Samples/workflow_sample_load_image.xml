<workflow xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                          xsi:noNamespaceSchemaLocation="../workflow/schemas/workflow_v1.xsd">
<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
	 This sample demonstrates the use of loading image with base64 format in workflow.
	 ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
--> 
    <autoTakeOwnership>true</autoTakeOwnership>
    <workflowInfo>
        <parallelSteps>false</parallelSteps>
        <workflowID scope="none">ZOS_WF</workflowID>
        <workflowDefaultName>Workflow Load Image Test</workflowDefaultName>
        <workflowDescription>The Workflow is used to test how to load image in workflow.</workflowDescription>
        <workflowVersion>1.0</workflowVersion>
        <vendor>IBM</vendor>
        <General/>
    </workflowInfo>
    <step name="loadBase64Image" optional="false">
        <title>A step to load image by base64 format</title>
        <description/>
        <instructions substitution="false">
            Load the image by using base64 encoding:
            <p>Before loading the image.</p>
            <![CDATA[
            <img width="150" height="150" src="data:image/png;base64," >
            ]]>
            <br/>
            <p>After loading the image.</p>
        </instructions>
        <weight>1</weight>
        <autoEnable>false</autoEnable>
        <canMarkAsFailed>false</canMarkAsFailed>
    </step>
</workflow>
