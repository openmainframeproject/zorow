//*  DEFINE DB2  CATALOG TABLE SPACE = SYSJAUXA
//* *******************************************************************
//SYSIN DD *
   DEFINE CLUSTER                             -
         ( NAME(${VCATALOG}.DSNDBC.DSNDB06.SYSJAUXA.I0001.A001) -
#if (${CATDDACL} != "")
           DATACLAS(${CATDDACL})     -
#end
#if (${CATDMGCL} != "")
           MGMTCLAS(${CATDMGCL})     -
#end
#if (${CATDSTCL} != "")
           STORCLAS(${CATDSTCL})     -
#end
           LINEAR                             -
           REUSE                              -
           KILOBYTES(660 660)                   -
           SHAREOPTIONS(3 3) )                -
      DATA                                    -
         ( NAME(${VCATALOG}.DSNDBD.DSNDB06.SYSJAUXA.I0001.A001) -
                              )    CATALOG(${VCATALOG})

   DEFINE CLUSTER                             -
