   DEFINE CLUSTER                             -
         ( NAME(${VCATALOG}.DSNDBC.DSNDB01.DSNSPT01.I0001.A001) -
#if (${CATXDACL} != "")
           DATACLAS(${CATXDACL})     -
#end
#if (${CATXMGCL} != "")
           MGMTCLAS(${CATXMGCL})     -
#end
#if (${CATXSTCL} != "")
           STORCLAS(${CATXSTCL})     -
#end
           LINEAR                             -
           REUSE                              -
