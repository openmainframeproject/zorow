           SHAREOPTIONS(3 3) )                -
      DATA                                    -
         ( NAME(${VCATALOG}.DSNDBD.DSNDB01.DSNSPT01.I0001.A001) -
                              )    CATALOG(${VCATALOG})

   DEFINE CLUSTER                             -
         ( NAME(${VCATALOG}.DSNDBC.DSNDB01.DSNSPT02.I0001.A001) -
#if (${CATXDACL} != "")
           DATACLAS(${CATXDACL})     -
#end
#if (${CATXMGCL} != "")
           MGMTCLAS(${CATXMGCL})     -
#end
#if (${CATXSTCL} != "")
           STORCLAS(${CATXSTCL})     -
#end
           LINEAR                             -
           REUSE                              -
           KILOBYTES(488 488)                   -
