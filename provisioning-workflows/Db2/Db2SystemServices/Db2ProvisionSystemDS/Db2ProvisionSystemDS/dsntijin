      DATA                                    -
         ( NAME(${VCATALOG}.DSNDBD.DSNDB01.SCT02.I0001.A001) -
                                 )    CATALOG(${VCATALOG})

   DEFINE CLUSTER                             -
         ( NAME(${VCATALOG}.DSNDBC.DSNDB01.DSNSCT02.I0001.A001) -
#if (${CATXDACL} != "")
           DATACLAS(${CATXDACL})     -
#end
#if (${CATXMGCL} != "")
